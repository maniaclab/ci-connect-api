{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "required": true,
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha1" ]
    },
    "memberships": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "state": {
            "type": "string",
            "enum": ["pending","active","admin","disabled"]
          },
          "state_set_by": {
            "type": "string"
          }
        },
        "required": ["name","state","state_set_by"]
      },
    }
  },
  "required": ["apiVersion","user_memberships"]
}
